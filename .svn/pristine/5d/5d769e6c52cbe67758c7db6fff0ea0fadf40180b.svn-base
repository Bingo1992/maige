@charset 'UTF-8';
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,p,th,td,header,hgroup,nav,section,article,aside,footer,menu,button,img,a,s {
    margin: 0;
    padding: 0;
    outline: 0;
}

button {
	border:none;
}

img {
    border: 0;
    vertical-align: middle;
    width: 100%;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}

ul,ol,dl {
    list-style: none;
}

a {
	text-decoration: none;
}

html,body {
   height: 100%;
}
    
body {
    font-size: 12px;
    font-family: arial, "Microsoft YaHei", simsun;
    line-height: 20px;
}

/*换屏样式*/
.slide {
    position: relative;
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.slide .content {
    width: 100%;
    height: 100%;
    margin: 0 auto;
}

.slide .content article {
    width: 100%;
    height: 100%;
    overflow: hidden;
    -webkit-background-size: cover;
    background-size: cover;
    /*line-height: 400px;*/
    /*text-align: center;*/
    color: #fff;
    font-size: 100px;
}

.slide .content article:nth-child(1) {
    background-image: url(../images/bg1.png);
}

.slide .content article:nth-child(2) {
    background-image: url(../images/bg2.png);
}

.slide .content article:nth-child(3) {
    background-image: url(../images/bg3.png);
}

.slide .content article:nth-child(4) {
    background-image: url(../images/bg4.png);
}



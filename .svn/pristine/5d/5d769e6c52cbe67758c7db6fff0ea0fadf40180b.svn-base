/*动画样式*/
@-webkit-keyframes translateA/* Safari and Chrome */
{
	0%   {-webkit-transform:translateX(-5rem);}
	100% {-webkit-transform:translateX(0);}
}

@-webkit-keyframes translateB/* Safari and Chrome */
{
	0%   {-webkit-transform:translateX(5rem);}
	100% {-webkit-transform:translateX(0);}
}

@-webkit-keyframes translateC/* Safari and Chrome */
{
	0%   {
		-webkit-transform:translateY(-10rem);
	}
	100% {
		-webkit-transform:translateY(0);
		top: 0;
	}
}

@-webkit-keyframes translateF
{
	0%   {
		-webkit-transform:translateY(-3rem) translateZ(0);
	}
	100% {
		-webkit-transform:translateY(0) translateZ(0);
		top: 1rem;
	}
}

@-webkit-keyframes translateD/* Safari and Chrome */
{
	0%   {
		-webkit-transform:translateX(16rem) translateY(-10rem) translateZ(-10rem) scale(0);
		/*-webkit-transform:scale(0.4);*/
	}

	100% {
		-webkit-transform:translateX(0) translateY(0) translateZ(0) scale(1);
		/*-webkit-transform:scale(1);*/
	}
}

@-webkit-keyframes opacity/* Safari and Chrome */
{
	0%   {opacity:0;}
	100% {opacity:1;}
}

@-webkit-keyframes scale/* Safari and Chrome */
{
	0%   {
		-webkit-transform:scale(0);
		opacity: 0;
	}
	100% {
		-webkit-transform:scale(1);
		opacity: 1;
	}
}

@-webkit-keyframes scaleB/* Safari and Chrome */
{
	0%   {
		-webkit-transform:scale(0);
		
	}
	100% {
		-webkit-transform:scale(1);
		
	}
}

@-webkit-keyframes scale_translateC/* Safari and Chrome */
{
	0%   {
		-webkit-transform:scale(0) translateY(20rem);
		opacity: 0;
	}

	100% {
		-webkit-transform:scale(1) translateY(0);
		opacity: 1;
	}
}

@-webkit-keyframes scale_translateA/* Safari and Chrome */
{
	0%   {
		-webkit-transform:scale(0) translateX(-10rem);
		opacity: 0;
	}
	100% {
		-webkit-transform:scale(1) translateX(0);
		opacity: 1;
	}
}

@-webkit-keyframes scale_translateB/* Safari and Chrome */
{
	0%   {
		-webkit-transform:scale(0) translateX(10rem);
		opacity: 0;
	}
	100% {
		-webkit-transform:scale(1) translateX(0);
		opacity: 1;
	}
}

@-webkit-keyframes translateE/* Safari and Chrome */
{
	0%   {-webkit-transform: translateY(1.5rem);opacity: 1;}
	100% {-webkit-transform: translateY(0);opacity: 0;}
}

@-webkit-keyframes rotateA/* Safari and Chrome */
{
	0%   {-webkit-transform:rotate(0);}
	100% {-webkit-transform:rotate(360deg);}
}